# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# home page
GET         /                     controllers.HomeController.listIssues
GET         /tmp/create/db        controllers.HomeController.tmpMethod

GET         /container            controllers.HomeController.container
GET         /list                 controllers.HomeController.listAjaxAsync
GET         /listGmcs             controllers.HomeController.listGmcs
GET         /listOrigins          controllers.HomeController.listOrigins

GET         /export               controllers.HomeController.export
GET         /send                 controllers.HomeController.sendNotifications
POST        /upload               controllers.HomeController.upload
POST        /status               controllers.HomeController.changeStatus

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file         controllers.Assets.versioned(path="/public", file: Asset)
