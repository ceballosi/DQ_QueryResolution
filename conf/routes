# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# home page
GET     /                                                   controllers.HomeController.listIssues
GET     /tmp/create/db                                      controllers.HomeController.tmpMethod

GET     /container                  controllers.HomeController.container
#GET     /list                       controllers.HomeController.listAjax
GET     /list                       controllers.HomeController.listAjaxAsync2

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
